  "author": "Moses Aweda <awedamoses@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MosesAweda/React-Spreadsheet-Editor.git"
  },
  "homepage": "https://github.com/MosesAweda/React-Spreadsheet-Editor#readme",
  "bugs": {
    "url": "https://github.com/MosesAweda/React-Spreadsheet-Editor/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
